{
    "mastodon": {
        "values": {
            "guid": "func uuid.New()",
            "status_type": "statuses.reblog_to_id : if statuses.reblog_to_id == nil: {return Status Message} else {return Reshare}",
            "commentable_type":"Post",
            "like_target_type":"Post"
        },
        "diaspora": {
            "accounts":[
                {
                    "users":{
                        "username":"username",
                        "serialized_private_key":"private_key",
                        "created_at":"created_at",
                        "updated_at":"updated_at"
                    },
                    "people":{
                        "id":"id",
                        "serialized_public_key":"public_key",
                        "diaspora_handle":"display_name"
                    },
                    "profiles":{
                        "bio":"note",
                        "image_url":"avatar_remote_url"
                    }
                }
            ],
            "users": [
                {
                    "people":{
                        "id":"account_id"
                    },
                    "users": {
                        "email":"email",
