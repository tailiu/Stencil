                        "updated_at":"updated_at"
                    }
                }
            ],
            "statuses": [
                {
                    "posts": {
                        "conditions":{
                            "visibility": "0",
                            "reply": "False"
                        },
                        "map":{
                            "id":"id",
                            "author_id":"account_id",
                            "text":"text",
                            "created_at":"created_at",
                            "updated_at":"updated_at",
                            "type": "#status_type",
                            "guid":"$guid"
                        }
                    }
                },
                {
                    "comments": {
                        "conditions":{
                            "visibility": "0",
                            "reply": "True"
                        },
                        "map":{
                            "id":"id",
                            "author_id":"account_id",
                            "commentable_id":"in_reply_to_id",
                            "text":"text",
                            "created_at":"created_at",
                            "updated_at":"updated_at",
                            "commentable_type": "$commentable_type",
                            "guid":"$guid"

                        }
                    }
                },
                {
                    "messages": {
                        "conditions":{
                            "visibility": "3"
                        },
                        "map":{    
                            "id":"id",
                            "author_id":"account_id",
                            "conversation_id":"conversation_id",
                            "text":"text",
                            "created_at":"created_at",
                            "updated_at":"updated_at",
                            "guid":"$guid"
                        }
                    }
                }
            ],
        "conversations": [
            {
                "conversations":{
                    "id":"id",
                    "created_at":"created_at",
                    "updated_at":"updated_at",
                    "guid":"$guid"
                }
            }
        ],
        "favourites": [
            {
                "likes":{
                    "id":"id",
                    "target_id":"status_id",
                    "created_at":"created_at",
